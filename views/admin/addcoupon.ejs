<script>
    function validateForm() {
        const code = document.getElementById("code").value;
        const startDate = new Date(document.getElementById("start_date").value);
        const expiryDate = new Date(document.getElementById("expiry_date").value);
        const amount = parseFloat(document.getElementById("amount").value);

        // Clear any previous error messages
        document.getElementById("codeError").textContent = "";
        document.getElementById("dateError").textContent = "";
        document.getElementById("expdateError").textContent = "";
        document.getElementById("amountError").textContent = "";

        if (code.trim() === "") {
            document.getElementById("codeError").textContent = "Coupon code cannot be empty.";
            return false;
        }

        if (startDate >= expiryDate) {
            document.getElementById("dateError").textContent = "Start date should be before the expiry date.";
            return false;
        }

        if (amount < 0) {
            document.getElementById("amountError").textContent = "Coupon discount cannot be negative.";
            return false;
        }

        // If validation is successful, show a success SweetAlert
        Swal.fire({
            title: 'Success!',
            text: 'Your coupon has been created successfully.',
            icon: 'success',
        });

        return true;
    }
</script>

 
