        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    <p></p>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody><% let j=1 %>
                                <% for(let i=orderData.length - 1;i >= 0;i--,j++){ %>
                                <tr>
                                    <td><%= i+1 %></td>
                                    <td><b><%= orderData[i].user_name  %></b></td>
                                    <td><template class="__cf_email__" data-cfemail="aec3cfdcd8c7c0eecbd6cfc3dec2cb80cdc1c3">[email&#160;protected]</template></td>
                                    <td><%= orderData[i].total_price  %></td>
                                    <td>
                                                            <span class="badge rounded-pill 
                                                              <% if (orderData[i].delevery_status === 'Delivered') { %>
                                                                  alert-success text-success
                                                              <% } else if (orderData[i].delevery_status === 'Shipped') { %>
                                                                  alert-warning text-warning
                                                                  <% } else if (orderData[i].delevery_status === 'Pending') { %>
                                                                    alert-pink text-orenge
                                                              <% } else if (orderData[i].delevery_status === 'Cancel') { %>
                                                                  alert-danger text-danger
                                                                  <% } else if (orderData[i].delevery_status === 'confirmed') { %>
                                                                    alert-orenge text-orenge
                                                                    <% } else if (orderData[i].delevery_status === 'Return') { %>
                                                                        alert-orenge text-danger
                                                              <% } else { %>
                                                                  alert-primary text-primary
                                                                 <% } %>"
                                                             ><%= orderData[i].delevery_status %></span>                                    </td>
                                    <td> <%= orderData[i].order_date.toLocaleDateString()  %></td>
                                    <td class="text-end">
                                        <a href="/admin/adminhome/orderdetail/?id=<%= orderData[i]._id   %>" class="btn btn-md rounded font-sm">Detail</a>
 
                                    </td>
                                </tr>
                            <% } %>
 
                                 
                          
                         
                      
                         
                        
                      
                        
                          
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
